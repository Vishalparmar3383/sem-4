    "### s1 = decimal and s2 = medical are Anagram"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 38,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Given String is not Anagram\n"
     ]
    }
   ],
   "source": [
    "s1 = input(\"Enter first string : \")\n",
    "s2 = input(\"Enter second string : \")\n",
    "if sorted(s1.replace(' ','').lower()) == sorted(s2.replace(' ','').lower()):\n",
    "    print(\"Given String is Anagram\")\n",
    "else :\n",
    "    print(\"Given String is not Anagram\")\n"
   ]
