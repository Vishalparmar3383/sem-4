   "source": [
    "### 07) WAP to capitalize the first and last character of each word in a string."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "old string : hi how\n",
      "new string : HI HoW\n"
     ]
    }
   ],
   "source": [
    "s = input(\"Enter String : \")\n",
    "ts = s.split()\n",
    "ts[0][1].upper\n",
    "for i in range(len(ts)) :\n",
    "    ts[i] = ts[i][0].upper() + ts[i][1:len(ts[i])-1] + ts[i][-1].upper()\n",
    "ans = ' '.join(ts)\n",
    "print(\"old string :\",s)\n",
    "print(\"new string :\",ans)"
   ]
