    "### 10) : Display credit card number. \n",
    "### card no. : 1234 5678 9012 3456\n",
    "### display as  : **** **** **** 3456"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "**** **** **** 1234\n"
     ]
    }
   ],
   "source": [
    "cn = input(\"Enter credit card number : \")\n",
    "ans = ''\n",
    "for i in range(len(cn)-4) :\n",
    "    if cn[i].isnumeric() :\n",
    "        ans += '*'\n",
    "    else :\n",
    "        ans += ' '\n",
    "ans += cn[-4:]\n",
    "print(ans)"
   ]
